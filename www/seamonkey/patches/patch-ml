Treat DragonFly like FreeBSD.

--- mozilla/js/src/Makefile.in.orig	2012-11-18 10:19:38.000000000 +0000
+++ mozilla/js/src/Makefile.in
@@ -670,6 +670,9 @@ endif # _MSC_VER
 ifeq ($(OS_ARCH),FreeBSD)
 EXTRA_LIBS	+= -pthread
 endif
+ifeq ($(OS_ARCH),DragonFly)
+EXTRA_LIBS	+= -pthread
+endif
 ifeq ($(OS_ARCH),Linux)
 EXTRA_LIBS	+= -ldl
 endif
